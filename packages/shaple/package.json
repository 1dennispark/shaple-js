{
  "name": "@shaple/shaple",
  "version": "0.0.1",
  "description": "This is javascript client libraries for Shaple",
  "main": "dist/main/index.js",
  "scripts": {
    "build": "run-s clean format build:*",
    "build:module": "tsc -p tsconfig.module.json",
    "build:main": "tsc -p tsconfig.json",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "clean": "rimraf dist"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "keywords": [
    "Shaple",
    "javascript",
    "typescript"
  ],
  "module": "dist/module/index.js",
  "types": "dist/module/index.d.ts",
  "dependencies": {
    "@supabase/auth-js": "^2.62.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
