  "homepage": "https://github.com/paust-team/shaple-js",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "module": "dist/module/index.js",
  "types": "dist/module/index.d.ts",
  "dependencies": {
    "@supabase/auth-js": "^2.62.1"
